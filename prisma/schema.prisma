generator client {
  provider = "prisma-client-js"
}

datasource db {
  provider = "postgresql"
  url      = env("APP_DB_URL")
}

model User {
  id Int @id @default(autoincrement())

  createdAt DateTime @default(now()) @map("created_at")
  updateAt  DateTime @updatedAt @map("updated_at")

  email    String @unique
  password String

  name  String @unique
  phone String @default("")
}
